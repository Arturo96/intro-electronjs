{
	"name": "intro-electronjs",
	"productName": "Electron App Starter",
	"version": "1.0.0",
	"description": "",
	"main": "src/index.js",
	"scripts": {
		"start": "electron src/index.js",
		"package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
		"package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
		"package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Arturo96/intro-electronjs.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Arturo96/intro-electronjs/issues"
	},
	"homepage": "https://github.com/Arturo96/intro-electronjs#readme",
	"dependencies": {
		"bootstrap": "^4.5.0",
		"electron": "^9.1.1",
		"electron-packager": "^15.0.0",
		"electron-reload": "^1.5.0",
		"jquery": "^3.5.1",
		"popper.js": "^1.16.1"
	}
}
